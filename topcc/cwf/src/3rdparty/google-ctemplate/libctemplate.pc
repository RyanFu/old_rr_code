prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: ctemplate
Version: 0.99
Description: Simple but powerful template language for C++
URL: http://code.google.com/p/google-ctemplate
Requires:
Libs: -L${libdir} -lctemplate
Libs.private: -pthread 
Cflags: -I${includedir}
