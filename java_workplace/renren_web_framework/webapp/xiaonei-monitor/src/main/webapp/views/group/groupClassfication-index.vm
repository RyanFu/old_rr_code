    <body id="security" class="security width-auto">
        <div id="header" class="clearfix">
            <h1 class="logo">
                <a title="XNMonitor" href="http://monitor.d.xiaonei.com/">
                    <img src="http://xnimg.cn/webmonitor/img/logo.png" />
                </a>
            </h1>
            <div class="menu-body clearfix">
                <ul class="main-menu">
                </ul>
                <ul class="sub-menu">
                	 Renren 
                </ul>
            </div>

        </div>
        <div id="main">
        	<div class="group box">
        		<div class="box-hd">
        			<strong class="title">现在已有的GROUP</strong><span class="lightgray">（不能重复创建，对于已有的Group你只能修改）</span>
                    <a href="createGroup" class="create-btn"><span>创建GROUP</span></a>
        		</div>
				<div class="box-bd clearfix">
						#foreach($group in ${renrenGroupList})
					<div class="smallbox">

						<div class="smallbox-hd">$group.groupName</div>
						<div class="smallbox-bd"><a href="groupAndDomains?groupName=$group.groupName">Detail</a></div>
					</div>
					#end
				</div>
        	</div>
            <div class="domain box">
            	<div class="box-hd">
            		<strong class="title">还没认领的域名</strong>
            	</div>
				<div class="box-bd clearfix">
					#foreach($ungroupedDomain in ${ungroupedDomainsList})
					<div class="smallbox">
						<div class="smallbox-hd">$ungroupedDomain</div>
					</div>
						#end
				</div>

            </div>
        </div>
