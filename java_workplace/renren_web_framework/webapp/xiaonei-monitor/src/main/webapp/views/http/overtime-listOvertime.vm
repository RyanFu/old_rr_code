    <body id="security" class="security width-auto">
        <div id="header" class="clearfix">
            <h1 class="logo">
                <a title="XNMonitor" href="http://monitor.d.xiaonei.com/">
                    <img src="http://xnimg.cn/webmonitor/img/logo.png" />
                </a>
            </h1>
            <div class="menu-body clearfix">
                <ul class="main-menu">
                </ul>
                <ul class="sub-menu">
                </ul>
            </div>
        </div>
        <div id="main">
            <div class="monitor box">
                <div class="box-hd">
                    <form id="search">
                        <select name="dayType" onChange="submit()">
                            <option value="hour" 
                                #if($dayType=="hour") 
                                    selected=true
                                #end>
                                过去一个小时
                            </option>
                            <option value="day" 
                                #if($dayType=="day") 
                                    selected=true
                                #end>
                                过去一天
                            </option>
                            <option value="week" 
                                #if($dayType=="week") 
                                    selected=true
                                #end>
                                过去一周
                            </option>
                            <option value="month" 
                                #if($dayType=="month") 
                                    selected=true
                                #end>
                                过去一月
                            </option>
                        </select>
                        <select name="groupName" onChange="submit()">
                            <option value="all"
                                #if($groupName=="all")
                                    selected=true
                                #end
                                >All
                            </option>
                            #foreach($renrenGroup in ${groupsList})
                                <option value=$renrenGroup.groupName
                                    #if($groupName==$renrenGroup.groupName)
                                        selected=true
                                    #end
                                    >$renrenGroup.groupName
                                </option>
                            #end
                            <option value="other"
                                #if($groupName=="other")
                                    selected=true
                                #end
                                >Others
                            </option>
                        </select>
                        <input type="hidden" value="$dsName" name="dsName"/>
                        <strong class="title">
                            <a href="/http/overtime/listOvertime" class="current">HTTP 监控</a>
                            <a href="/db/sql_Jrobin/listBiz">SQL 监控</a>
                            <a href="/http/error/httpErr">404/500</a>
                            <a href="/im/im/imInfoList">IM状况</a>
                        </strong>
                    </form>
                    <!--
                    <span class="back">
                    <a href="#">&laquo;返回</a>
                    <span class="select">
                    <a href="#" id="fold"></a>
                    </span>
                    </span>
                    <ul class="selects" id="selects" style="display:none;">
                    <li>
                    <a href="#">SQL监控</a>
                    </li>
                    <li>
                    <a href="#">&lt;返回上两级</a>
                    </li>
                    </ul>
                    -->
                    <script type="text/javascript">
                        var oLink = document.getElementById("fold");
                        var selects = document.getElementById("selects");
                        var st;
                        oLink.onmouseover = function(){
                        selects.style.display = "block";
                        this.className = "hover";
                        }
                        oLink.onmouseout = function(){
                        st = setTimeout(function(){
                        selects.style.display = "none";
                        oLink.className = "";
                        },200)
                        }
                        selects.onmouseover = function(){
                        clearTimeout(st);
                        this.style.display = "block";
                        oLink.className = "hover";
                        }
                        selects.onmouseout = function(){
                        this.style.display = "none";
                        oLink.className = "";
                        }
                    </script>
                </div>
                <div class="box-bd">
                    <div class="box-content">
                        <ul class="figure">
                            #foreach ($rrdFile in ${rrdFiles})
                                <li>
                                    <a href="listOvertimeDetail?groupName=$groupName&dayType=$dayType&childDir=$rrdFile">
                                        <img src="../../imgstream/getImg?rrdFile=$rrdFile&dsName=$dsName&logType=httplog&dayType=$dayType" style="border:0"/>
                                    </a>
									<br/>
                                    <a href="listOvertimeSep?groupName=$groupName&rrdFile=$rrdFile&dsName=$dsName&logType=httplog&dayType=$dayType">详情图</a>
                                </li>
                            #end
                        </ul>
                    </div>
                </div>
            </div>
