${"\r"}<#assign typeText="张照片"><#compress><#include "1|0|0"><li id="feed${id[0]}">	<#include "1|4|1">	<h3>		<a href="http://www.renren.com/profile.do?id=${f_from_id[0]}&ref=${vType}" stats="NF_People" target="_blank">${f_from_name[0]}</a>		<#if f_album_photo_digest?? && f_album_photo_digest[0] != "">			: ${f_album_photo_digest[0]}					<#else>			的照片 <a href="http://photo.renren.com/getalbum.do?id=${f_album_id[0]}&owner=${f_from_id[0]}&ref=${vType}" target="_blank">${f_album_title[0]}</a>		</#if>	</h3>	<div class="content">		<a href="http://photo.renren.com/getphoto.do?id=${f_album_photo_id[0]}&owner=${f_from_id[0]}&ref=${vType}" target="_blank"><img lala="${f_album_photo_image[0]}" src="http://s.xnimg.cn/a.gif" onload="feed_img_delay_load(this);" class="photo"/></a>	</div>	<div class="details">		<div class="legend">			<span class="duration"><@format>${f_time[0]}</@format></span>			<a href="#nogo" id="replyKey${f_album_photo_id[0]}" onClick="getReplyOfTheDoing('${f_album_photo_id[0]}','${f_from_id[0]}','${userID[0]}','f',false,'1','','photo')">				<span id="replyCount${f_album_photo_id[0]}"></span>			</a>			<#if isNewsFeed>			<span class="seperator">|</span>			<a href="#nogo" stats="NF_Share" onclick="create_share_feed(${f_album_photo_id[0]},${f_from_id[0]},'photo');" >分享</a>			</#if>			<span class="seperator">|</span>			<#if selfzancount??>				<a href="#nogo" stats="NF_Like" id="ILike${f_album_photo_id[0]}" onclick="ILike_toggleUserLike('photo','${f_album_photo_id[0]}','${userID[0]}','${f_from_id[0]}','${userName[0]}')" >不赞</a>			<#else>				<a href="#nogo" stats="NF_Like" id="ILike${f_album_photo_id[0]}" onclick="ILike_toggleUserLike('photo','${f_album_photo_id[0]}','${userID[0]}','${f_from_id[0]}','${userName[0]}')" class="ilike_icon" >赞</a>			</#if>		</div>		<div id="feedbody${f_album_photo_id[0]}" style="display:none;" class="replies"></div>		<script language="Javascript" status='1'> 			(function()			{				var reply = {"replyList":[					<#list f_reply_id as id>						<#if 2==id_index><#break></#if>						{"id":"${id}","replyer_tinyurl":"${f_reply_from_tinyimg[id_index]}","replyContent":${f_reply_body[id_index]},"ubname":"${f_reply_from_name[id_index]}","replyTime":"${f_reply_time[id_index]}","type":'${f_reply_type[id_index]}',"reply_layer":0,"ubid":${f_reply_from_id[id_index]}}						<#if (f_reply_id?size > 1) && 0==id_index>,</#if>					</#list>					],"ownerid":${f_from_id[0]},"userDigged":"${selfzancount[0]}","digged":"${totalzancount[0]}","type":"photo","fin":"${feedIndex[0]}"};				getReplyOfDoingFromJSON( reply , '${f_album_photo_id[0]}' ,'${userID[0]}' , '${reply_count[0]}','1' ,'','',true)			})();		</script>	</div><#include "1|1|1"></li></#compress>