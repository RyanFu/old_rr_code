${"\r"}<#assign typeText="个相册"><#compress><#include "1|0|0"><li id="feed${id[0]}">	<#include "1|4|1">	<h3>
		<#assign totalCount=0>
		<#list f_album_count as num>
			<#assign totalCount=totalCount+(num?number)>
		</#list>		<a href="http://www.renren.com/profile.do?id=${f_from_id[0]}&ref=${vType}" stats="NF_People" target="_blank">${f_from_name[0]}</a>		上传了${totalCount}张照片至 		<a href="http://photo.renren.com/getalbum.do?id=${f_album_id[0]}&owner=${f_from_id[0]}&ref=${vType}" target="_blank">${f_album_title[0]}</a>	</h3>	<div id="moreThumbnails${id[0]}" class="content">		<#list f_album_photo_image as aSrc>			<#if 3==aSrc_index><#break></#if>			<a href="http://photo.renren.com/getphoto.do?id=${f_album_photo_id[aSrc_index]}&owner=${f_from_id[0]}&ref=${vType}" target="_blank"><img width="100" src="${aSrc}" class="photo"/></a>		</#list>		<#assign photoIDs=""/>		<#list f_album_photo_id as aPid>				<#if aPid_index ==20><#break></#if>				<#assign photoIDs=photoIDs + aPid +","/>		</#list>		<span style="display: none;" id="morePicFeed${id[0]}">${photoIDs}</span>
		<#if (totalCount>3) && (photoIDs!="")>			<a href="#nogo" class="more" onclick="moreFeedPic('${id[0]}','${f_from_id[0]}')">查看更多</a>		</#if>	</div>	<div class="details">		<div class="legend">			<span class="duration"><@format>${f_time[0]}</@format></span>			<#include "1|5|1">			<a href="#nogo" id="replyKey${f_album_id[0]}" onClick="getReplyOfTheDoing('${f_album_id[0]}','${f_from_id[0]}','${userID[0]}','f',false,'2','','album')">				<span id="replyCount${f_album_id[0]}"></span>			</a>			<#if isNewsFeed>			<span class="seperator">|</span>			<a href="#nogo" stats="NF_Share" onclick="create_share_feed(${f_album_id[0]},${f_from_id[0]},'album');" >分享</a>			</#if>			<span class="seperator">|</span>				<#if selfzancount??>					<a href="#nogo" stats="NF_Like" id="ILike${f_album_id[0]}" onclick="ILike_toggleUserLike('album','${f_album_id[0]}','${userID[0]}','${f_from_id[0]}','${userName[0]}')" >不赞</a>					<#else>					<a href="#nogo" stats="NF_Like" id="ILike${f_album_id[0]}" onclick="ILike_toggleUserLike('album','${f_album_id[0]}','${userID[0]}','${f_from_id[0]}','${userName[0]}')" class="ilike_icon">赞</a>				</#if>		</div>		<div id="feedbody${f_album_id[0]}" style="display:none;" class="replies"></div>		<script language="Javascript" status='1'> 			(function()			{				var reply = {"replyList":[					<#list f_reply_id as id>						<#if 2==id_index><#break></#if>						{"id":"${id}","replyer_tinyurl":"${f_reply_from_tinyimg[id_index]}","replyContent":${f_reply_body[id_index]},"ubname":"${f_reply_from_name[id_index]}","replyTime":"${f_reply_time[id_index]}","type":'${f_reply_type[id_index]}',"reply_layer":0,"ubid":${f_reply_from_id[id_index]}}						<#if (f_reply_id?size > 1) && 0==id_index>,</#if>					</#list>					],"ownerid":${f_from_id[0]},"userDigged":"${selfzancount[0]}","digged":"${totalzancount[0]}","type":"album","fin":"${feedIndex[0]}"};				getReplyOfDoingFromJSON( reply, '${f_album_id[0]}' ,'${userID[0]}' , '${reply_count[0]}','2','','',true )
			})();		</script>	</div><#include "1|1|1"></li></#compress>