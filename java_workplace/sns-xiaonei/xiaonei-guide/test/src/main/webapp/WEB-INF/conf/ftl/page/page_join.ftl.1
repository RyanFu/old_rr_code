${"\r"}<#compress><#assign page = "1"/><#assign argu = "user_add"/><#include "1|0|0"><li id="feed${id[0]}"><#include "1|4|1">	<h3>		<#list f_from_name as n>			<a target="_blank"  href="http://www.renren.com/profile.do?id=${f_from_id[n_index]}&ref=${vType}&feed=${argu}" stats="NF_People">${n}</a>		</#list>		在人人成为了 <a href="http://page.renren.com/${f_page_id[0]}?ref=${vType}&feed=${argu}" target="_blank">${f_page_name[0]}</a> 		<#if f_page_certify?exists><img lala="http://xnimg.cn/imgpro/icons/follow.gif" src="http://s.xnimg.cn/a.gif" onload="feed_img_delay_load(this);" title="通过人人真实身份验证"/></#if>的<#if f_page_freestype_name[0] != "">${f_page_freestype_name[0]}<#else>粉丝</#if>	</h3>	<#if f_page_image[0] != "">		<div class="content">			<div class="figure"><a href="http://page.renren.com/${f_page_id[0]}?ref=${vType}&feed=${argu}" target="_blank"><img class="photo" lala="${ (f_page_image[0])!"about:blank"}" src="http://s.xnimg.cn/a.gif" onload="feed_img_delay_load(this);"/></a></div>			<#if f_page_digest[0] != ""><p class="comment"><q>${f_page_digest[0]}</q></p></#if>		</div>	</#if>	<div class="details">		<div class="legend">			<span class="duration"><@format>${f_time[0]}</@format></span>			<#include "1|5|1">			<a href="http://page.renren.com/pa/bf?pid=${f_page_id[0]}&ref=${vType}&feed=${argu}" target="_blank">			<#if f_page_freestype_action[0] != "">				${f_page_freestype_action[0]}			<#else>				成为粉丝			</#if>			</a>		</div>	</div><#include "1|1|1"></li></#compress>