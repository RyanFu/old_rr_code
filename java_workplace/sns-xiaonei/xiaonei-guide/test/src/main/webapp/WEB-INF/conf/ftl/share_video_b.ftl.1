${"\r"}
<#compress>
<#include "1|0|0">
<li id="feed${id[0]}">
<#include "1|4|1">
	<h3>
		<a href="http://www.renren.com/profile.do?id=${f_from_id[0]}&ref=${vType}" stats="NF_People" target="_blank">${f_from_name[0]}</a> 分享视频 <a target="_blank" href="http://share.renren.com/share/GetShare.do?id=${f_share_id[0]}&owner=${f_from_id[0]}&ref=${vType}">${f_share_video_title[0]}</a>
	</h3>
	<div class="content">
		<div style="background-image: url(${f_share_video_image[0]});" class="video">
			<a onclick="newsfeed_play_video('feed${id[0]}','${f_share_video_url[0]}', ${f_share_video_scale[0]});" href="#nogo">播放</a>
		</div>
		<#if f_share_comment?? && f_share_comment[0] != ""><p class="comment"><q>${f_share_comment[0]}</q></p></#if>
		<div class="v-share-tips" style="display: none;">
			<div class="share-button">
				<div class="tip-arrow"> </div>
				<p class="tip-disc">马上分享给好友？</p>
				<p class="v-share-btn">
					<input type="button" value="分享" class="input-btn" onclick="newsfeed_share_video('feed${id[0]}', '${f_share_id[0]}', '${f_from_id[0]}');"/>
				</p>
				<p class="tip-time"><em class="timer">5</em>秒后自动消失</p>
			</div>
			<p style="display: none;" class="tip-success"><img src="http://xnimg.cn/imgpro/icons/yes-v2.png"/>分享成功</p>
			<p style="display: none;" class="share-loading">正在发送请求...</p>
		</div>
	</div>
	<div class="content">
		<div class="video-play" style="display: none;">
			<div class="player">
			</div>
			<span class="retract"><a onclick="newsfeed_close_video('feed${id[0]}')" href="#nogo"><img class="retract-icon" src="http://xnimg.cn/a.gif"/>收起视频</a></span>
			<#if f_share_comment[0] != "">
				<p class="comment">
					<q>${f_share_comment[0]}</q>
				</p>
			</#if>
			<div class="v-operations">
				<#if selfzancount??>
					<a href="#nogo" stats="NF_Like" class="de-ilike-link"  id="XILike${f_share_id[0]}" onclick="ILike_toggleUserLike('share','${f_share_id[0]}','${userID[0]}','${f_from_id[0]}','${userName[0]}')" >不赞</a>
				<#else>
					<a href="#nogo" stats="NF_Like" class="ilike-link" id="XILike${f_share_id[0]}"  onclick="ILike_toggleUserLike('share','${f_share_id[0]}','${userID[0]}','${f_from_id[0]}','${userName[0]}')" >赞</a>
				</#if>
				<a class="share-link" stats="NF_Share" onclick="create_share_div(${f_share_id[0]},${f_from_id[0]},${f_from_id[0]});" href="#nogo">分享</a>
			</div>
		</div>
	</div>
	<div class="details">
		<div class="legend">
			<span class="duration"><@format>${f_time[0]}</@format></span>
			<a href="#nogo" id="replyKey${f_share_id[0]}" onClick="getReplyOfTheDoing('${f_share_id[0]}','${f_from_id[0]}','${userID[0]}','f',false,'4')">
				<span id="replycommentCount${f_share_id[0]}"></span>
			</a>
			<span class="seperator">|</span>
			<a href="#nogo" stats="NF_Share" onclick="create_share_div(${f_share_id[0]},${f_from_id[0]},${f_from_id[0]});" >分享</a>
			<span class="seperator">|</span>
				<#if selfzancount??>
					<a href="#nogo" stats="NF_Like" id="ILike${f_share_id[0]}" onclick="ILike_toggleUserLike('share','${f_share_id[0]}','${userID[0]}','${f_from_id[0]}','${userName[0]}')" >不赞</a>
				<#else>
					<a href="#nogo" stats="NF_Like" class="ilike_icon" id="ILike${f_share_id[0]}" onclick="ILike_toggleUserLike('share','${f_share_id[0]}','${userID[0]}','${f_from_id[0]}','${userName[0]}')" >赞</a>
				</#if>
		</div>
		<div id="media${f_share_id[0]}">
		</div>
		<div id="feedbody${f_share_id[0]}" style="display:none;" class="replies"></div>
		<script language="Javascript" status='1'> 
			(function()
			{
				var reply = {"replyList":[
					<#list f_reply_id as id>
						<#if 2==id_index><#break></#if>
						{"id":"${id}","replyer_tinyurl":"${f_reply_from_tinyimg[id_index]}","replyContent":${f_reply_body[id_index]},"ubname":"${f_reply_from_name[id_index]}","replyTime":"${f_reply_time[id_index]}","type":0,"reply_layer":0,"ubid":${f_reply_from_id[id_index]}}
						<#if (f_reply_id?size > 1) && 0==id_index>,</#if>
					</#list>
					],"ownerid":${f_from_id[0]},"userDigged":"${selfzancount[0]}","digged":"${totalzancount[0]}","type":"share","guestName":"${userName[0]}","fin":"${feedIndex[0]}"};
				getReplyOfDoingFromJSON( reply , '${f_share_id[0]}' ,'${userID[0]}' , '${reply_count[0]}','4','','',true)
			})();
		</script>
	</div>
<#include "1|1|1">
</li>
</#compress>