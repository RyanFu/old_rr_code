${"\r"}
<#compress>
<#include "1|0|0">
<li id="feed${id[0]}">
        <#include "1|4|1">
        <h3><a href="http://www.renren.com/profile.do?id=${f_from_id[0]}&ref=${vType}" stats="NF_People" target="_blank">${f_from_name[0]}</a> 提问 <a href="${f_qa_url[0]}">${f_qa_title[0]}</a>
        </h3>
        <#if f_qa_body?? && f_qa_body[0] != "">
           <div class="content">
                <blockquote>${f_qa_body[0]}</blockquote>
           </div>
        </#if>
        <div class="details">
                <div class="legend">
                    <span class="duration"><@format>${f_time[0]}</@format></span>
                </div>
        </div>
<#include "1|1|1">
</li>
</#compress>