${"\r"}<#assign typeText="篇日志"><#compress><#include "1|0|0"><li id="feed${id[0]}">	<#include "1|4|1">	<h3>		<a href="http://www.renren.com/profile.do?id=${f_from_id[0]}&ref=${vType}" stats="NF_People" target="_blank">${f_from_name[0]}</a>		发表日志 <a href="http://blog.renren.com/GetEntry.do?id=${f_blog_id[0]}&owner=${f_from_id[0]}&ref=${vType}" target="_blank" id="title${id[0]}">${f_blog_title[0]}</a>		<#if f_blog_type?? && f_blog_type[0] == "1">			<a href="http://i.renren.com/privilege/letter.action?wc=210000" target="_blank"><img lala="http://s.xnimg.cn/imgpro/editor/letter.gif" src="http://s.xnimg.cn/a.gif" onload="feed_img_delay_load(this);"/></a>		</#if>	</h3>	<div class="content">		<#if f_blog_video_image??>			<div class="blogMedia"> 				<div style="background-image: url(${f_blog_video_image[0]});" class="video">					<a target="_blank" href="${f_blog_url[0]}">播放</a>				</div>			</div>		</#if>		<#if f_blog_photo_image??>			<div class="blogMedia">				<a target="_blank" href="${f_blog_url[0]}"><img width="100" class="photo" src="${f_blog_photo_image[0]}"/></a>			</div>		</#if>		<#if f_blog_music??>			<div class="blogMedia">				<div class="audio playmedia">					<a target="_blank" href="${f_blog_url[0]}" class="pl-btn">播放音乐</a>				</div>			</div>		</#if>		<blockquote>${f_blog_digest[0]}</blockquote>	</div>	<div class="details">		<div class="legend">			<span class="duration"><@format>${f_time[0]}</@format></span>			<#include "1|5|1">			<a href="#nogo" id="replyKey${f_blog_id[0]}" onClick="getReplyOfTheDoing('${f_blog_id[0]}','${f_from_id[0]}','${userID[0]}','f',false,'0','','blog')">				<span id="replyCount${f_blog_id[0]}"></span>			</a>			<#if isNewsFeed>				<span class="seperator">|</span>				<a href="#nogo" stats="NF_Share" onclick="create_share_feed(${f_blog_id[0]},${f_from_id[0]},'blog');" >分享</a>			</#if>			<span class="seperator">|</span>				<#if selfzancount??>					<a href="#nogo" stats="NF_Like" id="ILike${f_blog_id[0]}" onclick="ILike_toggleUserLike('blog','${f_blog_id[0]}','${userID[0]}','${f_from_id[0]}','${userName[0]}')" >不赞</a>				<#else>					<a href="#nogo" stats="NF_Like" class="ilike_icon" id="ILike${f_blog_id[0]}" onclick="ILike_toggleUserLike('blog','${f_blog_id[0]}','${userID[0]}','${f_from_id[0]}','${userName[0]}')" >赞</a>				</#if>		</div>		<div id="feedbody${f_blog_id[0]}" style="display:none;" class="replies"></div>		<script language="Javascript" status='1'> 				(function()				{					var reply = {"replyList":[						<#list f_reply_id as id>							<#if 2==id_index><#break></#if>							{"id":"${id}","replyer_tinyurl":"${f_reply_from_tinyimg[id_index]}","replyContent":${f_reply_body[id_index]},"ubname":"${f_reply_from_name[id_index]}","replyTime":"${f_reply_time[id_index]}","type":'${f_reply_type[id_index]}',"reply_layer":0,"ubid":"${f_reply_from_id[id_index]}"}							<#if (f_reply_id?size > 1) && 0==id_index>,</#if>						</#list>						],"ownerid":${f_from_id[0]},"userDigged":"${selfzancount[0]}","digged":"${totalzancount[0]}","type":"blog","guestName":"${userName[0]}","fin":"${feedIndex[0]}"};					getReplyOfDoingFromJSON( reply , '${f_blog_id[0]}' ,'${userID[0]}' , '${reply_count[0]}','0' ,'','',true)				})();		</script>	</div><#include "1|1|1"></li></#compress>