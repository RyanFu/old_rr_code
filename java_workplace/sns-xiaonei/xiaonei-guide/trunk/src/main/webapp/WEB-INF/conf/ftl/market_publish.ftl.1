${"\r"}
<#compress>
<#include "1|0|0">
<li id="feed${id[0]}">
<#include "1|4|1">
	<h3>
		<a href="http://www.renren.com/profile.do?id=${f_from_id[0]}&ref=${vType}" stats="NF_People" target="_blank">${f_from_name[0]}</a> 发布集市信息 <a href="${f_market_url[0]}&ref=${vType}" target="_blank">${f_market_title[0]}</a>
	</h3>
	<div class="content">
		<#if f_market_image?? && f_market_image[0] != "">
			<a href="${f_market_url[0]}&ref=${vType}" target="_blank"><img class="photo" lala="${(f_market_image[0])!"about:blank"}" src="http://s.xnimg.cn/a.gif" onload="feed_img_delay_load(this);"/></a>
		</#if>
		<p>来自: <a target="_blank" href="http://www.renren.com/profile.do?id=${f_market_owner_id[0]}&ref=${vType}" stats="NF_People">${f_market_owner_name[0]}</a> </p>
		<p>${f_market_digest[0]}</p>
	</div>
	<div class="details">
		<div class="legend">
			<span class="duration"><@format>${f_time[0]}</@format></span>
		</div>
	</div>
<#include "1|1|1">
</li>
</#compress>