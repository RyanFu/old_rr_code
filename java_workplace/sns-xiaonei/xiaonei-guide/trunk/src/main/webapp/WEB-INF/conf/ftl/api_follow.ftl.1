${"\r"}
<#compress>
<#include "1|0|0">
<li id="feed${id[0]}">
	<#include "1|4|1">
	<h3>
		<#list f_from_name as n>
			<a target="_blank" href="http://www.renren.com/profile.do?id=${f_from_id[n_index]}&ref=${vType}" stats="NF_People">${n}</a>
		</#list> 在人人网成为了 <a href="http://page.renren.com/application/${f_app_id[0]}?ref=${vType}" target="_blank">${f_app_title[0]}</a> 的关注者
	</h3>
	<#if f_app_digest?? && f_app_digest[0] != "">
		<div class="content">
			<#if f_app_image?? && f_app_image[0] != "">
				<div class="figure">
					<a href="http://page.renren.com/application/${f_app_id[0]}?ref=${vType}" target="_blank"><img lala="${f_app_image[0]}" src="http://s.xnimg.cn/a.gif" onload="feed_img_delay_load(this);"/></a>
				</div>
			</#if>
			<p>${f_app_digest[0]}</p>
		</div>
	</#if>
	<div class="details">
		<div class="legend">
			<span class="duration"><@format>${f_time[0]}</@format></span>
			<a href="http://page.renren.com/application/${f_app_id[0]}?ref=${vType}" target="_blank">成为关注者</a>
		</div>
	</div>
<#include "1|1|1">
</li>
</#compress>