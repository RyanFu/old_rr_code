${"\r"}
<#compress>
<#include "1|0|0">
<li id="feed${id[0]}">
	<#include "1|4|1">
	<h3><a href="http://www.renren.com/profile.do?id=${f_from_id[0]}" target="_blank" stats="NF_People">${f_from_name[0]}</a>${f_app_title[0]}</h3>
	<div class="content">
		<#if f_app_image?? && f_app_image[0] != "">
			<div class="figure">
				<a href="${f_app_url[0]}" target="_blank"><img class="photo" width="100" lala="${f_app_image[0]}" src="http://s.xnimg.cn/a.gif" onload="feed_img_delay_load(this);"/></a>
			</div>
		</#if>
		<#if f_app_describe?? && f_app_describe[0] != "">
			<p style="color:#515151;margin:0 0 5px 0px;">${f_app_describe[0]}</p>
		</#if>
		<#if f_app_body?? && f_app_body[0] != "">
			<p style="margin-left:0px;">${f_app_body[0]}</p>
		</#if>
	</div>

	<div class="details">
		<div class="legend">
			<span class="duration"><@format>${f_time[0]}</@format></span>
			<#if (f_app_id?exists)&&(f_app_id[0]!="0")>
				<a href="#nogo" id="replyKey${f_app_id[0]}" onClick="getReplyOfTheDoing('${f_app_id[0]}','${f_from_id[0]}','${userID[0]}','f',false,'0','','blog')">
					<span id="replyCount${f_app_id[0]}"></span>
				</a>
				<span class="seperator">|</span> 
			</#if>
			<#if f_app_button_url?? &&  f_app_button_url[0] != "">
				<a href="${f_app_button_url[0]}" target="_blank">${f_app_button_title[0]}</a>
			</#if>
		</div>
	<#if (f_app_id?exists)&&(f_app_id[0]!="0")>
	
		<div id="feedbody${f_app_id[0]}" style="display:none;" class="replies"></div>
		
		<script language="Javascript" status='1'> 
				(function()
				{
					var reply = {"replyList":[
						<#list f_reply_id as id>
							<#if 2==id_index><#break></#if>
							{"id":"${id}","replyer_tinyurl":"${f_reply_from_tinyimg[id_index]}","replyContent":${f_reply_body[id_index]},"ubname":"${f_reply_from_name[id_index]}","replyTime":"${f_reply_time[id_index]}","type":0,"reply_layer":0,"ubid":"${f_reply_from_id[id_index]}"}
							<#if (f_reply_id?size > 1) && 0==id_index>,</#if>
						</#list>
						],"ownerid":${f_from_id[0]},"userDigged":"${selfzancount[0]}","digged":"${totalzancount[0]}","type":"app","guestName":"${userName[0]}","fin":"${feedIndex[0]}"};
					getReplyOfDoingFromJSON( reply , '${f_app_id[0]}' ,'${userID[0]}' , '${reply_count[0]}','6' ,'','',true)
				})();
		</script>
	</#if>
	</div>
 
<#include "1|1|1">
</li>
</#compress>