${"\r"}
<#compress>
<#assign page = "1"/>
<#assign argu = "page_photo"/>
<#assign num = 0/>
<#list f_album_count as c>
	<#assign num=num+c?number>
</#list>
<#include "1|0|0">
<li id="feed${id[0]}">
	<div class="figure">
		<a href="http://page.renren.com/${f_from_id[0]}?ref=${vType}&feed=${argu}&tagid=${f_album_id[0]}" stats="NF_People" target="_blank"><img needclip="1" width="50" height="50" lala="${(f_from_tinyimg[0])!"about:blank"}" src="http://s.xnimg.cn/a.gif" onload="feed_img_delay_load(this);" /></a>
	</div>
	<h3>
		<a href="http://www.renren.com/profile.do?id=${f_from_id[0]}&ref=${vType}&feed=${argu}&tagid=${f_album_id[0]}" target="_blank">${f_from_name[0]}</a>
		<#if f_from_certify?exists><img lala="http://xnimg.cn/imgpro/icons/follow.gif" src="http://s.xnimg.cn/a.gif" onload="feed_img_delay_load(this);" title="通过人人真实身份验证"/></#if>
		上传了${num}张照片至 <a href="http://page.renren.com/${f_from_id[0]}/album/${f_album_id[0]}?ref=${vType}&feed=${argu}&tagid=${f_album_id[0]}" target="_blank">${f_album_title[0]}</a>
	</h3>
	<div id="moreThumbnails${id[0]}" class="content">
		<#list f_album_photo_image as aSrc>
			<#if 3==aSrc_index><#break></#if>
			<a href="http://page.renren.com/${f_from_id[0]}/photo/${f_album_photo_id[aSrc_index]}?ref=${vType}&feed=${argu}&tagid=${f_album_id[0]}" target="_blank"><img width="100" lala="${aSrc}" src="http://s.xnimg.cn/a.gif" onload="feed_img_delay_load(this);" class="photo"/></a>
		</#list>
	</div>
	<div class="details">
		<div class="legend">
			<span class="duration"><@format>${f_time[0]}</@format></span>
			<#include "1|5|1">
		</div>
	</div>
<#include "1|1|1">
</li>
</#compress>
