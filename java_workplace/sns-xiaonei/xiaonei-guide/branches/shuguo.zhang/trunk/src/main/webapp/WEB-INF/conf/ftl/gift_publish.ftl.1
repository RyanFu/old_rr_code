${"\r"}
<#compress>
<#include "1|0|0">
<li id="feed${id[0]}">
	<#include "1|4|1">
	<h3>
		<a href="http://www.renren.com/profile.do?id=${f_from_id[0]}&ref=${vType}" stats="NF_People" target="_blank">${f_from_name[0]}</a> 收到了
		<#if f_gift_sender_name[0] == "一个TA">
			一个TA
		<#elseif f_gift_sender_name[0] == "神秘的TA">
			神秘的TA
		<#else>
			<a href="http://www.renren.com/profile.do?id=${f_gift_sender_id[0]}&ref=${vType}" stats="NF_People" target="_blank">${f_gift_sender_name[0]}</a>
		</#if>
		送的 <a href="${f_gift_url[0]}&ref=${vType}" target="_blank">${f_gift_title[0]}</a>
		<#if f_gift_body[0] != "">
			${f_gift_body[0]}
		</#if>
	</h3>
	<div class="content">
		<div class="figure"><a href="${f_gift_url[0]}&ref=${vType}" target="_blank"><img lala="${(f_gift_image[0])!"about:blank"}" src="http://s.xnimg.cn/a.gif" onload="feed_img_delay_load(this);" /></a></div>
		<#if f_gift_sender_name[0] != "一个TA" && f_gift_digest[0] != ""><p class="comment"><q>${f_gift_digest[0]}</q></p></#if>
		<#if f_gift_ad_title[0] != ""><p class="giftlink">${f_gift_ad_title[0]}</p></#if>
	</div>
	<div class="details">
		<div class="legend">
			<span class="duration"><@format>${f_time[0]}</@format></span>
			<#include "1|5|1">
			<a href="http://gift.renren.com/send.do?fid=${f_from_id[0]}" target="_blank">更多精彩礼物&raquo;</a>
		</div>
	</div>
<#include "1|1|1">
</li>
</#compress>
