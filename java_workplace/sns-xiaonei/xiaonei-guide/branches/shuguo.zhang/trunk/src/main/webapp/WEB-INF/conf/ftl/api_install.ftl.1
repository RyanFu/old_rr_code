${"\r"}
<#compress>
<#include "1|0|0">
<li id="feed${id[0]}">
	<#include "1|4|1">
	<h3>
		<#list f_from_name as n>
			<a target="_blank" href="http://www.renren.com/profile.do?id=${f_from_id[n_index]}&ref=${vType}" stats="NF_People">${n}</a>
		</#list> 添加应用 <a href="${f_app_url[0]}&ref=${vType}" target="_blank">${f_app_title[0]}</a>
	</h3>
	<#if f_app_digest?? && f_app_digest[0] != "">
		<div class="content">
			<div class="figure">
				<a href="${f_app_url[0]}&ref=${vType}" target="_blank"><img class="photo" lala="${f_app_image[0]}" src="http://s.xnimg.cn/a.gif" onload="feed_img_delay_load(this);"/></a>
			</div>
			<blockquote>${f_app_digest[0]}</blockquote>
		</div>
	</#if>
	<div class="details">
		<div class="legend">
			<span class="duration"><@format>${f_time[0]}</@format></span>
			<a href="${f_app_about_url[0]}&ref=${vType}" target="_blank">查看应用详情</a>
		</div>
	</div>
<#include "1|1|1">
</li>
</#compress>
