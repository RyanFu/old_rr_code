${"\r"}
<#compress>
<#assign page = "1"/>
<#assign argu = "org_regossip"/>
<#include "1|0|0">
	<li id="feed${id[0]}">
		<div class="figure">
			<a class="avatar" href="http://org.renren.com/${f_from_id[0]}?ref=${vType}&feed=${argu}" stats="NF_People" target="_blank"><img needclip="1" width="50" height="50" lala="${(f_from_tinyimg[0])!"about:blank"}" src="http://s.xnimg.cn/a.gif" onload="feed_img_delay_load(this);" /></a>
		</div>
		<h3>
			<a href="http://org.renren.com/${f_from_id[0]}?ref=${vType}&feed=${argu}" stats="NF_People" target="_blank">${f_from_name[0]}</a>
			在 <a href="${f_gossip_owner_url[0]}&s=gossip&ref=${vType}&feed=${argu}" target="_blank">留言板</a> 回复了关注者
		</h3>
		<div class="content">
			<#if f_gossip_digest?exists && f_gossip_digest[0] != ""><div class="pagecomment">${f_gossip_digest[0]}</div></#if>
		</div>
		<div class="details">
			<div class="legend">
				<span class="duration"><@format>${f_time[0]}</@format></span>
			</div>
		</div>
<#include "1|1|1">
	</li>
</#compress>
