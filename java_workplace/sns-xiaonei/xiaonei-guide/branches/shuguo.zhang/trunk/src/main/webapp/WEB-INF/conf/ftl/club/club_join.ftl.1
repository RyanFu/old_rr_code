${"\r"}
<#compress>
<#assign page = "1"/>
<#assign argu = "orguser_add"/>
<#include "1|0|0">
<li id="feed${id[0]}">
	<div class="figure">
		<a class="avatar" href="http://www.renren.com/profile.do?id=${f_from_id[0]}&ref=${vType}&feed=${argu}" stats="NF_People" target="_blank"><img needclip="1" width="50" height="50" lala="${(f_from_tinyimg[0])!"about:blank"}" src="http://s.xnimg.cn/a.gif" onload="feed_img_delay_load(this);" /></a>
	</div>
	<h3>
		<#list f_from_name as n>
			<a target="_blank" stats="NF_People" href="http://www.renren.com/profile.do?id=${f_from_id[n_index]}&ref=${vType}&feed=${argu}">${n}</a>
		</#list>
		加入了俱乐部 <a href="http://org.renren.com/${f_page_id[0]}?ref=${vType}&feed=${argu}" target="_blank">${f_page_name[0]}</a> 
	</h3>
	<div class="content">
		<div class="figure"><a href="http://org.renren.com/${f_page_id[0]}?ref=${vType}&feed=${argu}" target="_blank"><img class="photo" lala="${ (f_page_image[0])!"about:blank"}" src="http://s.xnimg.cn/a.gif" onload="feed_img_delay_load(this);"/></a></div>
		<#if f_page_digest?exists && f_page_digest[0] != ""><p class="comment"><q>${f_page_digest[0]}</q></p></#if>
	</div>
	<div class="details">
		<div class="legend">
			<span class="duration"><@format>${f_time[0]}</@format></span>
			<a href="http://org.renren.com/pa/bf?pid=${f_page_id[0]}&ref=${vType}&feed=${argu}" target="_blank">${f_page_freestype_action[0]}</a>
		</div>
	</div>
<#include "1|1|1">
</li>
</#compress>
