${"\r"}<#assign typeText="条状态"><#compress>	<#include "1|0|0">	<li id="feed${id[0]}">		<#include "1|4|1">		<h3>			<a href="http://www.renren.com/profile.do?id=${f_from_id[0]}&ref=${vType}" stats="NF_People" target="_blank">${f_from_name[0]}</a>			: ${f_status_title[0]}		</h3>		<div class="details">			<div class="legend">				<span class="duration"><@format>${f_status_time[0]}</@format></span>				<#include "1|5|1">				<a href="#nogo" id="replyKey${f_status_id[0]}" onClick="getReplyOfTheDoing('${f_status_id[0]}','${f_from_id[0]}','${userID[0]}','f',false,'3','','status')">					<span style="display:none" id="replyCount${f_status_id[0]}"></span>				</a> 				<#if isNewsFeed>					<span class="seperator">|</span>					<a href="#nogo" stats="NF_Fwd" onclick="forwardDoing('${f_status_id[0]}', '${f_from_id[0]}');" >转发</a>				<#elseif userID[0] != interviewee[0]>					<span class="seperator">|</span>					<a href="#nogo" stats="NF_Fwd" onclick="a_fowardDoing('${f_status_id[0]}', '${f_from_id[0]}');return false;">转发</a>				</#if>			</div>			<div id="feedbody${f_status_id[0]}" style="display:none;" class="replies"></div>			<script language="Javascript" status='1'> 				<#if f_reply_owner??>					(function()					{						var reply = {"replyList":[						<#list f_reply_id as id>							<#if 2==id_index><#break></#if>							{"id":"${id}","replyer_tinyurl":"${f_reply_tinyimg[id_index]}","replyContent":${f_reply_body[id_index]},"ubname":"${f_reply_name[id_index]}","replyTime":"${f_reply_time[id_index]}","type":0,"reply_layer":0,"ubid":"${f_reply_id[id_index]}"}							<#if (f_reply_id?size > 1) && 0==id_index>,</#if>						</#list>						],"ownerid":${f_from_id[0]},"type":"status","fin":"${feedIndex[0]}"};						getReplyOfDoingFromJSON( reply , '${f_status_id[0]}' ,'${userID[0]}' , '${reply_count[0]}','3','','',true)					})();				<#else>					(function()						{						var reply = {"replyList":[						<#list f_reply_id as id>						<#if 2==id_index><#break></#if>						{"id":"${id}","replyer_tinyurl":"${f_reply_from_tinyimg[id_index]}","replyContent":${f_reply_body[id_index]},"ubname":"${f_reply_from_name[id_index]}","replyTime":"${f_reply_time[id_index]}","type":'${f_reply_type[id_index]}',"reply_layer":0,"ubid":"${f_reply_from_id[id_index]}"}						<#if (f_reply_id?size > 1) && 0==id_index>,</#if>						</#list>						],"ownerid":${f_from_id[0]},"type":"status","fin":"${feedIndex[0]}"};						getReplyOfDoingFromJSON( reply , '${f_status_id[0]}' ,'${userID[0]}' , '${reply_count[0]}','3','','',true)					})();				</#if>				<#if isNewsFeed>					renderStatusFeed('feed${id[0]}','http://status.renren.com/getdoing.do?id=${f_from_id[0]}&doingId=${f_status_id[0]}&ref=${vType}');				</#if>			</script>		</div>	<#include "1|1|1">	</li></#compress>